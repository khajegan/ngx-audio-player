    "build:package": "npm run build:lib && npm run build:pack",
    "myScope": "cd projects/ngx-audio-player && sudo npm init --scope=@khajegan",
    "myBuild": "cd projects/ngx-audio-player &&  sudo ng b --configuration production",
    "myPublish": "cd dist/ngx-audio-player && npm publish --access public"
